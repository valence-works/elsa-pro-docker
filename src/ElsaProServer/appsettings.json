{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "CShells.AspNetCore.Notifications.ShellEndpointRegistrationHandler": "Warning",
      "ElsaProServer": "Warning",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore.Model.Validation": "Error"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "Elsa": "Data Source=elsa.db"
  },
  "Elsa": {
    "Shell": {
      "Features": [
        "AdminUser",
        "DefaultAuthentication",
        {
          "Name": "SqliteWorkflowPersistence",
          "ConnectionString": "Data Source=elsa.db;Cache=Shared"
        },
        {
          "Name": "SqliteIdentityPersistence",
          "ConnectionString": "Data Source=elsa.db;Cache=Shared"
        },
        {
          "Name": "FastEndpoints",
          "EndpointRoutePrefix": "elsa/api"
        }
      ],
      "Configuration": {
        "Identity": {
          "SigningKey": "CHANGE_ME_TO_A_SECURE_RANDOM_KEY"
        }
      }
    },
    "Cors": {
      "AllowedOrigins": [
        "http://localhost:5001",
        "https://localhost:5001"
      ]
    }
  }
}
